        {
          "pattern": ".json",
          "destination": "${workspaceFolder}/SPMV_AIE/build/hw_emu/SPMV_AIE_hw_emu_new"
        },
        {
          "pattern": ".csv",
          "destination": "${workspaceFolder}/SPMV_AIE/build/hw_emu/SPMV_AIE_hw_emu_new"
        }
      ],
      "pathMap": [
