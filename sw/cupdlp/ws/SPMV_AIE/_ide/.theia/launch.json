          "source": "/home/oa321/work/mudkip/sw/cupdlp/ws/SPMV_AIE/build/hw_emu/hw_link/spmv_aie/spmv_aie/int/xo/mm2s/mm2s/cpu_sources",
          "destination": "/home/oa321/work/mudkip/vivado/ws/aie_sys_design_mm2s/src"
        },
        {
          "source": "/home/oa321/work/mudkip/sw/cupdlp/ws/SPMV_AIE/build/hw_emu/hw_link/spmv_aie/spmv_aie/int/xo/s2mm/s2mm/cpu_sources",
          "destination": "/home/oa321/work/mudkip/vivado/ws/aie_sys_design_s2mm/src"
